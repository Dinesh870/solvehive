<style>
    body {
        font-family: Arial, sans-serif;
        background-color: #f0f2f5;
        color: #333;
        margin: 0;
        padding: 0;
        text-align: center;
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh;
    }

    .error-container {
        background-color: #b9f6cd;
        border: 1px solid #ddd;
        border-radius: 8px;
        padding: 40px;
        max-width: 600px;
        width: 90%;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    }

    .error-container h1 {
        font-size: 72px;
        margin: 0;
        color: #dc3545;
        /* Bootstrap's danger color */
    }

    .error-container .success {
        font-size: 35px;
        margin: 10px 0;
        color: green;
    }

    .error-container a {
        color: #007bff;
        /* Bootstrap's primary color */
        text-decoration: none;
        font-weight: bold;
    }

    .error-container a:hover {
        text-decoration: underline;
    }
</style>

<body>
    <div class="error-container">
        <h2 class="success">
            ðŸ˜€<%= message %>
        </h2>
        <h2>
            Look Up Here:
            <% 
                const path = typeof href !== 'undefined' ? href : '/' 
                const goMsg = typeof pathMsg !== 'undefined' ? pathMsg : 'go to home page'; 
            %>
            <a href= "<%= path %>">
                <%= goMsg %>
            </a>
            <% //} %>
        </h2>
    </div>
</body>